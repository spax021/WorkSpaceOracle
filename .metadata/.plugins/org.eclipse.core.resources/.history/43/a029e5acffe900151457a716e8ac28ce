package l2languageBasics;

public class Main {

	public static void main(String[] args) {
		
		int index = 0;
		int broj = 100;
		int[] rej = new int[10];
		
		for(int i=0; i<=9; i++){
			
			rej[i] =+ broj;
			System.out.println("Rej sa indexom " + i + " ima vrednost " + rej[i]);
		}
		System.out.println(rej[7]);
		
	}

}
