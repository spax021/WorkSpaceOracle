package l6Objects;

public class Spil {

    public static int znakovi = 4;
    public static int brojevi = 13;
    public static int brojKarata = znakovi * brojevi;

    private Karta[][] karte;

    public Spil() {
    	karte = new Karta[znakovi][brojevi];
        for (int znak = Karta.KARO; znak <= Karta.TREF; znak++) {
            for (int broj = Karta.KEC; broj <= Karta.KRALJ; broj++) {
                karte[znak-1][broj-1] = new Karta(broj, znak);
            }
        }
    }

    public Karta getKarta(int znak, int broj) {
        return karte[znak-1][broj-1];
    }
}
