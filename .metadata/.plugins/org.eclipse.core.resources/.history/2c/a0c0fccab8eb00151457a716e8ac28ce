package l6Objects;

public class Main {
//	Consider the following class:
    public static int x = 7; 
    public int y = 3; 
	
//    What are the class variables?  	PUBLIC INT Y
//    What are the instance variables?	PUBLIC STATIC INT X
    
   public static void main(String[] args) {
		
	   Main a = new Main();
	   Main b = new Main();
	   a.y = 5;
	   b.y = 6;
	   a.x = 1;
	   b.x = 2;
	   System.out.println("a.y = " + a.y);
	   System.out.println("b.y = " + b.y);
	   System.out.println("a.x = " + a.x);
	   System.out.println("b.x = " + b.x);
	   System.out.println("IdentifyMyParts.x = " + Main.x);

		
		
	}

}
